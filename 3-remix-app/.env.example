# Firebase Configuration (Public - safe to expose)
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# Firebase config as JSON (alternative to individual fields)
FIREBASE_CONFIG={"apiKey":"...","authDomain":"...","projectId":"...","storageBucket":"...","messagingSenderId":"...","appId":"..."}

# Server-side secrets (DO NOT expose to client)
JWT_PRIVATE_KEY=your_jwt_private_key_here
FIREBASE_SERVICE_ACCOUNT_KEY=your_service_account_json_here

# ML Service Configuration
ML_SERVICE_URL=http://localhost:8000

# Google AI (optional)
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key
GOOGLE_GENERATIVE_AI_MODEL_NAME=gemini-1.5-flash

# App Configuration
APP_NAME=Titanic ML Predictor
NODE_ENV=development
LOG_LEVEL=info